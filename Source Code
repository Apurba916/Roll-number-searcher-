#include <stdio.h>

int roll_no_searcher(int roll_no[], int size, int target){
    int low =0;
    int high = size - 1;
    while (low <= high)
    {
        int mid = (high + low) / 2;
        if (roll_no[mid] == target)
        {
            return mid;
        }
        else if (roll_no[mid] > target)
        {
            high = mid - 1;
        }
        else
        {
            low = mid + 1;
        }
    }
    return -1;
}

int main(){
    int roll_no[] = {101, 102, 103, 104, 105, 106, 107, 108, 109, 110};
    int size = sizeof(roll_no)/sizeof(roll_no[0]);
    int target;
    printf("Enter the roll number to search: ");
    scanf("%d", &target);
    printf("Verifying roll number %d...\n", target);
    
    int index = roll_no_searcher(roll_no, size, target);
    if(index != -1){
        printf("Roll number %d found at index %d.\n", target, index);
    } 
    else {
        printf("Roll number %d not found.\n", target);
    }
    return 0;
}
